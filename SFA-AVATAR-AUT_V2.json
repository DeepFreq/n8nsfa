    {
      "parameters": {
        "jsCode": "// Procesar mensaje de Telegram con video + indicaciones\nconst messageData = $input.first().json;\nlet instructions = '';\n\n// Obtener caption o texto del mensaje\nif (messageData.message.caption) {\n  instructions = messageData.message.caption;\n} else if (messageData.message.text) {\n  instructions = messageData.message.text;\n} else {\n  instructions = 'Sin instrucciones específicas';\n}\n\n// Configuración para SFA con video overlay\nconst config = {\n  avatar_id_empleado: \"REEMPLAZAR_CON_AVATAR_ID_REAL\",\n  empleado_nombre: \"Facundo Gorosito\",\n  whatsapp_phone_id: \"REEMPLAZAR_CON_PHONE_ID_REAL\",\n  julieta_phone_number: \"549341XXXXXXX\", // Número de Julieta\n  video_instructions: instructions,\n  input_video_file_id: messageData.message.video.file_id,\n  user_id: messageData.message.from.id,\n  chat_id: messageData.message.chat.id,\n  timestamp: new Date().toISOString()\n};\n\nreturn { json: config };"
      },
      "id": "28dabe9b-526e-4e29-88d7-440850fa6b17",
