    {
      "parameters": {
        "jsCode": "// Extraer script generado por Gemini\nconst geminiResponse = $input.first().json;\nlet script = '';\n\ntry {\n  if (geminiResponse.candidates && geminiResponse.candidates[0]) {\n    script = geminiResponse.candidates[0].content.parts[0].text;\n  } else {\n    script = 'Error: No se pudo generar el script';\n  }\n} catch (error) {\n  script = 'Error procesando respuesta de Gemini';\n}\n\n// Limpiar script para avatar\nscript = script.replace(/\\n/g, ' ').trim();\n\n// Obtener datos previos\nconst previousData = $('Process Video Instructions').first().json;\n\nreturn {\n  json: {\n    ...previousData,\n    overlay_script: script\n  }\n};"
      },
